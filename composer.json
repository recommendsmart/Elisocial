{
    "name": "goalgorilla/social_template",
    "description": "Social project template for composer based Open Social projects.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "goalgorilla/open_social": "~12.0.0",
        "php": "^8.0",
        "monolog/monolog": "^2.3.5",
        "drupal/commerce": "^2.36",
        "drupal/slick": "^2.10",
        "drupal/facets": "^2.0",
        "drupal/bootstrap_basic_image_gallery": "^1.6",
        "drupal/block_visibility_conditions": "^2.1",
        "drupal/interval": "^1.12",
        "drupal/physical": "^1.3",
        "drupal/bootstrap_layout_builder": "^2.1",
        "drupal/bootstrap_styles": "^1.1",
        "drupal/layout_builder_blocks": "^1.1",
        "drupal/layout_builder_lock": "^1.4",
        "drupal/layout_builder_modal": "^1.2",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_builder_restrictions": "^2.19",
        "drupal/section_library": "^1.1",
        "drupal/entity": "^1.4",
        "drupal/image_delta_formatter": "^1.2",
        "drupal/inline_block_title_automatic": "^1.1",
        "drupal/media_library_form_element": "^2.0",
        "drupal/core_views_facets": "^2.0",
        "drupal/facets_pretty_paths": "^2.0",
        "drupal/media_entity_slideshow": "^2.5",
        "drupal/button_link": "^2.0",
        "drupal/commerce_product_tax": "^1.0",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/display_layout": "^1.0",
        "drupal/dynamic_layouts": "^2.0",
        "drupal/ds": "^3.18",
        "drupal/switch_page_theme": "^4.0",
        "drupal/dxpr_theme": "^5.2",
        "drupal/entity_reference_views": "^1.1@beta",
        "drupal/entity_browser": "^2.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/social": [
                "goalgorilla/open_social"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    }
}
